{
  "name": "@nguyen-t/whisper",
  "description": "A basic Node.js native module wrapper for Whisper.cpp written using NAPI-RS.",
  "keywords": [
    "whisper",
    "rust",
    "ggml"
  ],
  "author": "nguyen-t",
  "homepage": "https://github.com/nguyen-t/whisper#readme",
  "bugs": {
    "url": "https://github.com/nguyen-t/whisper/issues"
  },
  "version": "0.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "whisper",
    "triples": {}
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.17.0",
    "@types/node": "^20.10.6",
    "ava": "^5.1.1"
  },
  "ava": {
    "timeout": "3m",
    "files": [
      "!whisper/**"
    ]
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "universal": "napi universal",
    "version": "napi version"
  },
  "packageManager": "yarn@4.0.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nguyen-t/whisper.git"
  }
}
